#!/usr/bin/env bash
# Lists files and prints part after the first dash, or full name if no dash

for file in *; do
  # Skip if it's not a regular file (optional, remove if directories should be included too)
  [ -f "$file" ] || continue

  if [[ "$file" == *-* ]]; then
    # Print everything after the first dash
    echo "${file#*-}"
  else
    # Print full name if no dash
    echo "$file"
  fi
done

